{% extends 'base.html.twig' %}

{% block title %}BTS{% endblock %}

{% block content %}
    <div class="header">
  <h1>BTS</h1>
  <h4> 방탄소년단 </h4>
</div>

<div class="topnav">
  <br><br><br>
